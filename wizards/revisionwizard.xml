<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="revisionwizard_form_view">
			<field name="name">immo.wizard.revision.form</field>
			<field name="model">immo.wizard.revision</field>
			<field name="arch" type="xml">
				<form string="New revision">
				<group col="4">
				<field name="date_start_old_revision" string="Date start old revision" readonly="1"/>
				<field name="date_end_old_revision"  string="Date end old revision" readonly="1"/>
				</group>
					<group col="4">
						
						<field name="rent" placeholder="New rent" />
						<field name="charges" />
						<field name="date_start" />
					</group>
					<footer>
						<button name="validate_revision" type="object" string="Validate"
							class="oe_highlight" />
						or
						<button special="cancel" string="Cancel" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</openerp>